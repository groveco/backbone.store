  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "fetch-mock": "^7.3.9",
    "jest": "^22.4.0",
    "jest-junit": "^1.3.0",
    "jsdoc": "^3.6.2",
    "node-fetch": "^2.6.0",
    "sinon": "^1.17.2"
